{
	"name": "stlint-vscode-plugin",
	"displayName": "stlint-vscode-plugin",
	"description": "Plugin STLint for Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"keywords": [
		"lint",
		"linter",
		"stylus",
		"stylus-linter",
		"vscode",
		"plugin",
		"stlint"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"author": "Chupurnov Valeriy<chupurnov@gmail.com>",
	"repository": {
	  "type": "git",
	  "url": "https://github.com/stylus/stlint-vscode-plugin"
	},
	"bugs": {
	  "url": "https://github.com/stylus/stlint-vscode-plugin/issues"
	},
	"license": "MIT"
}
